<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.exampledemo.parsaniahardik.gpsdemocode">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BarGraph"
            android:label="@string/title_activity_bar_graph"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Today_Distance" />
        <activity android:name=".Monthly_data" />
        <activity
            android:name=".Total_distance"
            android:label="@string/title_activity_total_distance"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>